#include<stdio.h>
void  main(){
    int mat1[2][2],mat2[2][2],mat3[2][2],i,j,k;
    printf("Enter the first matrix:");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            scanf("%d",&mat1[i][j]);
        }
    }
     printf("Enter the second matrix:");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            scanf("%d",&mat2[i][j]);
        }
    }
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            mat3[i][j]=0;
            for(k=0;k<2;k++){
                mat3[i][j]=mat3[i][j]+mat1[i][k]*mat2[k][j];
            }
        }
    }
    printf("Printing matrix\n");
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            printf("%d\t",mat3[i][j]);
        }printf("\n");
    }
}
